function DestroyUI() 
	savety111:Destroy()
end
function Minimize()
	if game:GetService("CoreGui").Xeroxus.TopConnection.MainUIFrame.Visible == true then
		MinimizeInfo = TweenInfo.new(2)
		MinimizeTween = game:GetService("TweenService"):Create(game:GetService("CoreGui").Xeroxus.TopConnection.MainUIFrame,MinimizeInfo,{Size = UDim2.new(0, 580,0.0, 0)})
		MinimizeTween:Play()
		game:GetService("CoreGui").Xeroxus.TopConnection.MainUIFrame.Visible = false
	else
		MinimizeTween = game:GetService("TweenService"):Create(game:GetService("CoreGui").Xeroxus.TopConnection.MainUIFrame,MinimizeInfo,{Size = UDim2.new(0, 580,0.0, 340)})
		MinimizeTween:Play()
		game:GetService("CoreGui").Xeroxus.TopConnection.MainUIFrame.Visible = true
	end
end
game:GetService("CoreGui").Xeroxus.TopConnection.Text.Minimize.MouseButton1Click:Connect(function()
	Minimize()
end)
game:GetService("CoreGui").Xeroxus.TopConnection.Text.Destroy.MouseButton1Click:Connect(function()
	DestroyUI()
end)
randomstringloop()
